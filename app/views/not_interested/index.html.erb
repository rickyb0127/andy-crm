<h1>Not Interested</h1>

<table class="table">

  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Phone Number</th>
    <th>Address</th>
    <th>Edit</th>
    <th>Notes</th>
  </thead>

  <tbody>
    <tr>
    <% @clients.each do |client| %>
      <td><%= client.first_name %></td>
      <td><%= client.last_name %></td>
      <td><%= client.email %></td>
      <td><%= client.phone_number %></td>
      <td><%= client.address %></td>
      <td><%= link_to "Edit Client", edit_user_client_path(@user.id, client) %></td>
      <td><%= client.notes %></td>
    </tr>
    <% end %>
  </tbody>
</table>
