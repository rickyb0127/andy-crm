<%= form_for @user, url: sign_up_path do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</br>
  <%= f.label :email %>
  <%= f.email_field :email %>
</br>
  <%= f.label :password %>
  <%= f.password_field :password %>
</br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
</br>
  <%= f.label :company %>
  <%= f.text_field :company %>
</br>
  <%= f.submit "Sign Up" %>
<% end %>
