<h1>Edit Client</h1>

<%= form_for @client do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</br>
  <%= f.label :email %>
  <%= f.email_field :email %>
</br>
  <%= f.label :address %>
  <%= f.text_field :address %>
</br>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
</br>
  <%= f.label :is_home %>
  <%= f.check_box :is_home %>
</br>
  <%= f.label :is_interested %>
  <%= f.check_box :is_interested %>
</br>
  <%= f.submit 'Edit Client' %>
<% end %>
