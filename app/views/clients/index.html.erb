<h1>Listing Leads</h1>

<table class="table">

  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Phone Number</th>
    <th>Address</th>
    <th>Edit</th>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
    <tr>
      <td><%= client.first_name %></td>
      <td><%= client.last_name %></td>
      <td><%= client.email %></td>
      <td><%= client.phone_number %></td>
      <td><%= client.address %></td>
      <td><%= link_to "Edit Client", edit_client_path(client) %></td>
    </tr>
    <% end %>
  </tbody>
</table>

</br>

<%= link_to 'Create New Lead', new_client_path %>
